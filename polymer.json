{
  "entrypoint": "index.html",
  "shell": "src/app-shell.js",
  "sources": ["src/**/*", "assets/**/*", "package.json"],
  "fragments": [
    "src/elements/pages/not-found-page-view/not-found-page-view.js",
    "src/elements/pages/engagements/engagements-page-main.js",
    "src/elements/pages/micro-assessments/micro-assessments-page-main.js",
    "src/elements/pages/spot-checks/spot-checks-page-main.js",
    "src/elements/pages/audits/audits-page-main.js",
    "src/elements/pages/special-audits/special-audits-page-main.js",
    "src/elements/pages/staff-sc/staff-sc-page-main.js"
  ],
  "extraDependencies": [
    "manifest.json",
    "version.json",
    "upgrade-browser.html",
    "node_modules/@webcomponents/webcomponentsjs/**",
    "node_modules/@webcomponents/shadycss/apply-shim.min.js",
    "node_modules/web-animations-js/web-animations-next-lite.min.js",
    "node_modules/dayjs/dayjs.min.js",
    "node_modules/dayjs/plugin/utc.js"
  ],
  "lint": {
    "rules": ["polymer-3"],
    "ignoreWarnings": ["could-not-resolve-reference", "non-literal-import"]
  },
  "builds": [
    {
      "name": "esm-bundled",
      "browserCapabilities": ["es2015", "modules"],
      "js": {
        "minify": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "bundle": true,
      "addServiceWorker": true
    }
  ],
  "moduleResolution": "node",
  "npm": true
}
