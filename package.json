{
  "name": "fam",
  "version": "3.0.0",
  "description": "Financial Assurance Module, formerly known as Auditor Portal",
  "repository": {
    "type": "git",
    "url": "https://github.com/unicef/etools-auditor-portal.git"
  },
  "license": "BSD-3-Clause",
  "scripts": {
    "start": "tsc --watch --preserveWatchOutput & polymer serve -H 0.0.0.0 -p 8080",
    "build": "export NODE_OPTIONS=--max_old_space_size=4096 && npm run tsToJs && polymer build",
    "build:static": "npm run tsToJs && polymer build",
    "tsToJs": "tsc --skipLibCheck || true",
    "initialbuild": "tsc && npm run build:prpl-server && npm run build:static",
    "build:prpl-server": "polymer build --auto-base-path && gulp prpl-server",
    "serve": "prpl-server --root server/build",
    "serve:static": "polymer serve --port 5000 build/es6-bundled",
    "serve:prpl-server": "prpl-server --root server/build",
    "test": "npm run test:unit && npm run test:integration",
    "test:integration": "tsc && mocha test/integration --timeout=10000",
    "test:unit": "tsc && polymer test --module-resolution=node --npm",
    "test:sauce": "tsc && polymer test --module-resolution=node --npm -s 'windows 10/microsoftedge@17' -s 'macos 10.13/safari@11'",
    "test:regenerate_screenshots": "tsc && mocha test/integration/screenshots-baseline/regenerate.js --timeout=15000",
    "lint:eslint": "eslint --ext .ts . --ignore-path .gitignore",
    "format:eslint": "eslint --ext .ts . --fix --ignore-path .gitignore",
    "lint:prettier": "prettier \"**/*.ts\" --check --ignore-path .gitignore",
    "lint": "polymer lint || true && npm run lint:eslint",
    "format": "npm run format:eslint"
  },
  "dependencies": {
    "@polymer/app-layout": "^3.1.0",
    "@polymer/app-route": "^3.0.2",
    "@polymer/iron-flex-layout": "^3.0.1",
    "@polymer/iron-icons": "^3.0.1",
    "@polymer/iron-pages": "^3.0.1",
    "@polymer/iron-selector": "^3.0.1",
    "@polymer/paper-button": "^3.0.1",
    "@polymer/paper-card": "^3.0.1",
    "@polymer/paper-checkbox": "^3.1.0",
    "@polymer/paper-icon-button": "^3.0.2",
    "@polymer/paper-input": "^3.2.1",
    "@polymer/paper-item": "^3.0.1",
    "@polymer/paper-listbox": "^3.0.1",
    "@polymer/paper-menu-button": "^3.0.1",
    "@polymer/paper-ripple": "^3.0.1",
    "@polymer/paper-styles": "^3.0.1",
    "@polymer/paper-tabs": "^3.0.1",
    "@polymer/paper-toast": "^3.0.1",
    "@polymer/paper-tooltip": "^3.0.1",
    "@polymer/polymer": "^3.4.1",
    "@types/lodash-es": "^4.17.4",
    "@unicef-polymer/etools-ajax": "^4.2.0",
    "@unicef-polymer/etools-app-selector": "^3.1.6",
    "@unicef-polymer/etools-behaviors": "^3.0.4",
    "@unicef-polymer/etools-content-panel": "5.0.0-rc.2",
    "@unicef-polymer/etools-currency-amount-input": "^3.1.0",
    "@unicef-polymer/etools-date-time": "^2.2.1",
    "@unicef-polymer/etools-dialog": "5.0.0-rc.2",
    "@unicef-polymer/etools-dropdown": "3.7.6",
    "@unicef-polymer/etools-info-tooltip": "4.0.0-rc.4",
    "@unicef-polymer/etools-loading": "5.0.0-rc.2",
    "@unicef-polymer/etools-profile-dropdown": "6.0.0-rc.1",
    "@unicef-polymer/etools-upload": "^3.5.0",
    "@webcomponents/webcomponentsjs": "^2.6.0",
    "dayjs": "^1.10.6",
    "dexie": "^3.0.1",
    "lodash-es": "^4.17.21",
    "pwa-helpers": "^0.9.1",
    "web-animations-js": "^2.3.2"
  },
  "devDependencies": {
    "@polymer/decorators": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "@typescript-eslint/typescript-estree": "^3.2.0",
    "axe-core": "^3.0.0",
    "browser-capabilities": "^1.1.3",
    "del": "^3.0.0",
    "eslint": "^7.2.0",
    "eslint-config-google": "^0.13.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-html": "^6.0.1",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.10",
    "pixelmatch": "^4.0.2",
    "polymer-cli": "^1.9.3",
    "polyserve": "^0.27.0",
    "prettier": "^2.0.5",
    "typescript": "^3.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && tsc --skipLibCheck"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix"
    ]
  }
}
