  "description": "Financial Assurance Module, formerly known as Auditor Portal",
  "repository": {
    "type": "git",
    "url": "https://github.com/unicef/etools-auditor-portal.git"
  },
  "dependencies": {
    "@polymer/app-layout": "^3.0.0",
    "@polymer/polymer": "^3.1.0",
    "@polymer/iron-flex-layout": "^3.0.1",
    "@polymer/iron-icons": "^3.0.1",
    "@polymer/iron-pages": "^3.0.1",
    "@polymer/iron-selector": "^3.0.1",
    "@polymer/paper-button": "^3.0.1",
    "@polymer/paper-checkbox": "^3.1.0",
    "@polymer/paper-icon-button": "^3.0.2",
    "@polymer/paper-input": "^3.0.1",
    "@polymer/paper-item": "^3.0.1",
    "@polymer/paper-listbox": "^3.0.1",
    "@polymer/paper-menu-button": "^3.0.1",
    "@polymer/paper-ripple": "^3.0.1",
    "@polymer/paper-styles": "^3.0.1",
    "@polymer/paper-tabs": "^3.0.1",
    "@polymer/paper-toast": "^3.0.1",
    "@polymer/paper-tooltip": "^3.0.1",
    "@webcomponents/webcomponentsjs": "^2.2.4",
    "@unicef-polymer/etools-app-selector": "3.0.0-rc.1",
    "@unicef-polymer/etools-behaviors": "3.0.0-rc.1",
    "@unicef-polymer/etools-ajax": "4.0.0-rc.1",
    "@unicef-polymer/etools-profile-dropdown": "4.0.0-rc.1",
    "@unicef-polymer/etools-content-panel": "3.0.0-rc.1",
    "@unicef-polymer/etools-dropdown": "3.0.0-rc.1",
    "@unicef-polymer/etools-info-tooltip": "3.0.0-rc.1",
    "@unicef-polymer/etools-loading": "4.0.0-rc.1",
    "@unicef-polymer/etools-currency-amount-input": "3.0.0-rc.1",
    "@unicef-polymer/etools-date-time": "1.0.0-rc.1",
    "dexie": "^2.0.4",
    "lit-element": "^2.0.1",
    "pwa-helpers": "^0.9.0",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "@types/lodash-es": "^4.17.3",
    "lodash-es": "^4.17.11",
    "web-animations-js": "^2.3.1"
  },
  "devDependencies": {
    "@polymer/test-fixture": "^4.0.2",
    "axe-core": "^3.0.0",
    "chai": "^4.1.2",
    "del": "^3.0.0",
    "gulp": "^4.0.0",
    "gulp-rename": "^1.3.0",
    "gulp-replace": "^1.0.0",
    "mocha": "^5.2.0",
    "pixelmatch": "^4.0.2",
    "polymer-cli": "^1.9.3",
    "polyserve": "^0.27.0",
    "prpl-server": "^1.4.0",
    "puppeteer": "^1.5.0",
    "typescript": "^3.1.3",
    "wct-mocha": "^1.0.0",
    "@polymer/decorators": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^1.6.0",
    "@typescript-eslint/parser": "^1.6.0",
    "@typescript-eslint/typescript-estree": "^1.6.0",
    "browser-capabilities": "^1.1.3",
    "web-component-tester": "^6.9.0",
    "wct-headless": "^2.2.2",
    "wct-local": "^2.1.3"
  },
  "scripts": {
    "start": "gulp serve",
    "build": "export NODE_OPTIONS=--max_old_space_size=4096 && npm run tsToJs && polymer build",
    "build:static": "npm run tsToJs && polymer build",
    "tsToJs": "tsc --skipLibCheck",
    "tsEslint": "eslint src_ts/**",
    "initialbuild": "tsc && npm run build:prpl-server && npm run build:static",
    "build:prpl-server": "polymer build --auto-base-path && gulp prpl-server",
    "serve": "prpl-server --root server/build",
    "serve:static": "polymer serve --port 5000 build/es5-bundled",
    "serve:prpl-server": "prpl-server --root server/build",
    "test": "npm run test:unit && npm run test:integration",
    "test:integration": "tsc && mocha test/integration --timeout=10000",
    "test:unit": "tsc && polymer test --module-resolution=node --npm",
    "test:sauce": "tsc && polymer test --module-resolution=node --npm -s 'windows 10/microsoftedge@17' -s 'macos 10.13/safari@11'",
    "test:regenerate_screenshots": "tsc && mocha test/integration/screenshots-baseline/regenerate.js --timeout=15000"
  }
}
