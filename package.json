{
  "name": "fam",
  "version": "3.0.0",
  "description": "Financial Assurance Module, formerly known as Auditor Portal",
  "repository": {
    "type": "git",
    "url": "https://github.com/unicef/etools-auditor-portal.git"
  },
  "license": "BSD-3-Clause",
  "scripts": {
    "build": "export NODE_OPTIONS=--max_old_space_size=4096 && rollup -c rollup-prod.config.js",
    "tsToJs": "tsc --skipLibCheck || true",
    "start": "rm -rf src && rollup -c rollup-dev.config.js -w",
    "test": "npm run test:unit && npm run test:integration",
    "test:integration": "tsc && mocha test/integration --timeout=10000",
    "test:unit": "tsc && polymer test --module-resolution=node --npm",
    "test:sauce": "tsc && polymer test --module-resolution=node --npm -s 'windows 10/microsoftedge@17' -s 'macos 10.13/safari@11'",
    "test:regenerate_screenshots": "tsc && mocha test/integration/screenshots-baseline/regenerate.js --timeout=15000",
    "lint:eslint": "eslint --ext .ts . --ignore-path .gitignore",
    "format:eslint": "eslint --ext .ts . --fix --ignore-path .gitignore",
    "lint:prettier": "prettier \"**/*.ts\" --check --ignore-path .gitignore",
    "lint": "npm run lint:eslint",
    "format": "npm run format:eslint",
    "generate:icons": "npm --prefix node_modules/@unicef-polymer/etools-unicef run generate-icons -- --output=$(pwd)/assets/icons/ --icons=$(pwd)/assets/icons/icons.js",
    "rd:init": "relative-deps init",
    "rd:add": "relative-deps add",
    "rd:reload": "relative-deps",
    "rd:watch": "chokidar \"../../etools-polymer/**/*.js\" \"../../etools-polymer/**/**/*.js\" -c 'npm run rd:reload' --silent"
  },
  "dependencies": {
    "@types/lodash-es": "^4.17.7",
    "@unicef-polymer/etools-modules-common": "^5.0.0-rc.46",
    "@unicef-polymer/etools-piwik-analytics": "4.0.1-rc.3",
    "@unicef-polymer/etools-types": "^1.11.17",
    "@unicef-polymer/etools-unicef": "^1.0.0-rc.64",
    "@unicef-polymer/etools-utils": "^2.0.0-rc.6",
    "chokidar-cli": "^3.0.0",
    "dayjs": "^1.11.10",
    "dexie": "^3.2.4",
    "lit": "^2.7.5",
    "lit-element": "^2.5.1",
    "lit-translate": "^2.0.1",
    "lodash-es": "^4.17.21",
    "pwa-helpers": "^0.9.1",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.1",
    "relative-deps": "^1.0.7"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-dynamic-import-vars": "^2.1.0",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "@typescript-eslint/typescript-estree": "^5.6.0",
    "ajv": ">=6.12.6",
    "async": ">=2.6.4",
    "axe-core": "^3.0.0",
    "browser-capabilities": "^1.1.3",
    "decode-uri-component": ">=0.2.2",
    "del": "^3.0.0",
    "engine.io": ">=3.6.1",
    "eslint": "^7.32.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-prettier": "^4.0.0",
    "follow-redirects": ">=1.14.9",
    "husky": "^4.3.8",
    "json5": ">=2.2.3",
    "lint-staged": "^10.2.10",
    "minimatch": ">=3.1.2",
    "mout": ">=1.2.4",
    "node-fetch": ">=2.6.7",
    "path-parse": ">=1.0.7",
    "pixelmatch": "^4.0.2",
    "prettier": "^2.5.1",
    "qs": ">=6.5.3",
    "rollup": "^2.79.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-esbuild": "^6.1.0",
    "rollup-plugin-filesize": "^9.1.2",
    "rollup-plugin-license": "^3.2.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-serve": "^2.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-workbox": "^8.0.1",
    "semver-regex": ">=3.1.4",
    "shelljs": ">=0.8.5",
    "socket.io": ">=2.5.0",
    "socket.io-parser": ">=3.3.3",
    "svg-sprite": "^2.0.2",
    "thenify": ">=3.3.1",
    "typescript": "^4.5.3",
    "ua-parser-js": ">=1.0.33",
    "urijs": ">=1.19.11",
    "wd": ">=1.14.0"
  },
  "relativeDependencies": {}
}
